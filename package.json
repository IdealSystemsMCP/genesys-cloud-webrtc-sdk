{
  "name": "purecloud-webrtc-sdk",
  "version": "3.0.0",
  "description": "client for the interfacing with PureCloud WebRTC",
  "repository": "https://github.com/mypurecloud/purecloud-webrtc-sdk",
  "license": "MIT",
  "main": "src/client.js",
  "browser": "web/purecloud-webrtc-sdk.js",
  "files": [
    "src",
    "web/purecloud-webrtc-sdk.js"
  ],
  "pre-push": [
    "test"
  ],
  "scripts": {
    "build": "npx webpack",
    "build-sample": "./test/test-pages/build.sh",
    "unit-test": "NODE_ENV=test jest --runInBand",
    "pretest": "npm run build",
    "start": "stupid-server -s -p 8443",
    "test": "npm run lint && npm run unit-test",
    "watch-tests": "jest --watch --collectCoverage=false --runInBand",
    "lint": "npm run semistandard && npm run tslint",
    "tslint": "tslint --project . --config tslint.json",
    "semistandard": "semistandard",
    "semistandard:fix": "semistandard --fix"
  },
  "semistandard": {
    "ignore": [
      "/.coverage",
      "/scripts",
      "/test/test-pages/sdk-sample.js"
    ]
  },
  "babel": {
    "presets": [
      "env"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      },
      "production": {
        "plugins": []
      }
    }
  },
  "author": "",
  "dependencies": {
    "@types/webrtc": "0.0.25",
    "backoff-web": "^1.0.1",
    "purecloud-platform-client-v2": "^28.0.0",
    "purecloud-streaming-client": "^8.3.2",
    "safe-json-stringify": "^1.2.0",
    "stupid-server": "^0.2.2",
    "superagent": "^3.8.3",
    "uuid": "^3.3.2",
    "webpack-auto-inject-version": "^1.2.2",
    "webrtc-stats-gatherer": "^6.0.4",
    "wildemitter": "^1.2.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/nock": "^10.0.3",
    "@types/safe-json-stringify": "^1.1.0",
    "@types/sinon": "^7.0.13",
    "@types/superagent": "^4.1.1",
    "@types/uuid": "^3.4.4",
    "@types/ws": "^6.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.8.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.3",
    "gulp": "^4.0.0",
    "gulp-uglify": "^3.0.1",
    "gulp-util": "^3.0.8",
    "jest": "^24.8.0",
    "nock": "^9.3.0",
    "semistandard": "^12.0.1",
    "sinon": "^7.1.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-semistandard": "^8.0.1",
    "typescript": "^3.5.2",
    "uglify-js": "^3.4.6",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watch": "^1.0.2",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.4",
    "ws": "^7.0.0"
  },
  "false": {}
}
